{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "import/no-unresolved": 0,
    "import/no-extraneous-dependencies": 0,
    "import/extensions": 0,
    "react/jsx-filename-extension": 0,
    "no-underscore-dangle": 0,
    "no-param-reassign": 0,
    "no-restricted-syntax": 0,
    "react/prefer-stateless-function": 0,
    "no-unused-vars": 0,
  },
  "globals": {
    "window": true,
    "document": true,
    "describe": true,
    "it": true,
  }
}