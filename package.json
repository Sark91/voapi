{
  "name": "react-redux-simple-boilerplate",
  "version": "0.0.1",
  "main": "index.js",
  "author": "Filip KrÄ™cichwost",
  "license": "MIT",
  "scripts": {
    "eslint": "NODE_PATH=. ./node_modules/.bin/eslint --ignore-path .gitignore .",
    "test": "NODE_PATH=. ./node_modules/.bin/mocha --compilers js:babel-register test/* test/**/* test/setup.js",
    "build": "NODE_PATH=. PROD_ENV=1 NODE_ENV=production ./node_modules/.bin/webpack -p",
    "server": "NODE_PATH=. NODE_ENV=production node ./server/server.babel.js",
    "start": "npm run server",
    "eslint-dev": "NODE_PATH=. ./node_modules/.bin/esw --ignore-path .gitignore --watch .",
    "test-dev": "NODE_PATH=. npm run test -- --watch",
    "build-dev": "NODE_PATH=. NODE_ENV=dev ./node_modules/.bin/webpack --colors --watch",
    "server-dev": "NODE_PATH=. NODE_ENV=dev node ./server/server.babel.js",
    "start-dev": "concurrently --kill-others \"npm run eslint-dev\" \"npm run test-dev\" \"npm run build-dev\" \"npm run server-dev\""
  },
  "dependencies": {
    "babel-core": "^6.20.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.9",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "express-restify-mongoose": "^4.1.3",
    "fs": "0.0.1-security",
    "history": "^4.4.1",
    "immutable": "^3.8.1",
    "lodash": "^4.17.2",
    "module": "^1.2.5",
    "mongoose": "^4.7.6",
    "node-env-file": "^0.1.8",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "concurrently": "^3.1.0",
    "dirty-chai": "^1.2.2",
    "eslint": "^3.13.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.x.x",
    "eslint-plugin-react": "^6.8.0",
    "eslint-watch": "^2.1.14",
    "html-webpack-plugin": "^2.24.1",
    "mocha": "^3.2.0",
    "react-hot-loader": "^1.3.1",
    "supertest": "^2.0.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  }
}
